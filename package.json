{
	"name": "bay-lang",
	"displayName": "Bay Lang",
	"description": "Bay Lang VSCode extension",
	"publisher": "Bayrell",
	"version": "0.1.0",
	"main": "index.js",
	"author": "Ildar Bikmamatov <ildar@bayrell.org>",
	"license": "Apache-2.0",
	"homepage": "https://github.com/bayrell/web-lang",
	"repository": {
		"type": "git",
		"url": "https://github.com/bayrell/bay-lang-vscode"
	},
	"activationEvents": [
		"onLanguage:bay"
	],
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [{
			"id": "bay",
			"aliases": ["Bay Lang", "bay"],
			"extensions": ["bay"]
		}],
		"grammars": [{
			"language": "bay",
			"scopeName": "source.bay",
			"path": "./syntaxes/bay.tmLanguage.json"
		}]
	}
}
